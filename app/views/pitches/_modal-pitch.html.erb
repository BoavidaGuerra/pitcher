

<!-- Modal -->
<div class="modal fade" id="onboarding" tabindex="-1" role="dialog" aria-labelledby="onboardingTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Pitch Onboarding</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
          <%= render 'pitches/onboarding' %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary" data-dismiss="modal">Skip</button>
        <button type="button" class="btn btn-primary" disabled>Next step</button>
      </div>
    </div>
  </div>
</div>
